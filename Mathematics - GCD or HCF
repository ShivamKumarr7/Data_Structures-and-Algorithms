// Mathematics 
// GCD or HCF of 2 numbers 


#include<bits/stdc++.h>
using namespace std;
int gcd(int a,int b)
{
    if(b == 0)
    {
        return a;
    }
    return gcd(b,a%b);
}
int main()
{
    int a,b;
    cin>>a>>b;
    int res1 = gcd(a,b);
    cout<<res1<<"\n";
    int ans = __gcd(a,b);
    cout<<ans<<"\n";
    return 0;
}
