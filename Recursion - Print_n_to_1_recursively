// Recursion
// Q) Print n to 1 using recursion 


#include<bits/stdc++.h>
using namespace std;
void print_n_to_1_recursively(int n)
{
    if(n < 1)
    {
        return;
    }
    cout<<n<<" ";
    print_n_to_1_recursively(n-1);
}
int main()
{
    int n;
    cin>>n;
    print_n_to_1_recursively(n);
    return 0;
}
