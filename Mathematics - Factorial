// Mathematics
// Factorial of a number 

// Q) Given a number 'n' find the factorial of the number.

#include<bits/stdc++.h>
using namespace std;
long long facto(int n)
{
    long long res = 1;
    for(int i=1;i<=n;i++)
    {
        res = res * i;
    }
    return res;
}
int main()
{
    int n;
    cin>>n;
    cout<<facto(n);
    return 0;
}
