// RECURSION
// Q) Print 1 to n using recursion ?? 

#include<bits/stdc++.h>
using namespace std;
void print_1_to_n_recursively(int n)
{
    if(n < 1)
    {
        return;
    }
    print_1_to_n_recursively(n-1);
    cout<<n<<" ";
}
int main()
{
    int n;
    cin>>n;
    print_1_to_n_recursively(n);
    return 0;
}
